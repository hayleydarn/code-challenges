    "def persistence(n):\n",
    "    if n < 10:\n",
    "        return 0\n",
    "    s = str(n)\n",
    "    res = 1\n",
    "    for i in s:\n",
    "        res = res * int(i)\n",
    "    return persistence(res) + 1"
   ]
  }
